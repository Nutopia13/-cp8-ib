{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist/public",
    "functions": {
        "api/index.js": {
            "maxDuration": 10,
            "includeFiles": "dist/**"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ]
}